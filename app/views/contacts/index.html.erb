<p id="notice"><%= notice %></p>

<h1>Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Phone</th>
      <th>Address</th>
      <th>Email</th>
      <th>Lat</th>
      <th>Long</th>
      <th>Facebook</th>
      <th>Acerca</th>
      <th>Misión</th>
      <th>Visión</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.phone %></td>
        <td><%= contact.address %></td>
        <td><%= contact.email %></td>
        <td><%= contact.lat %></td>
        <td><%= contact.long %></td>
        <td><%= contact.facebook %></td>
        <td><%= contact.about %></td>
        <td><%= contact.mission %></td>
        <td><%= contact.vision %></td>
        <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
